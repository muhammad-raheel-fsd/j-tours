<!DOCTYPE html>
<html lang="en">
   <%- include('../partials/head.ejs')%>
   <body>

      
      <!-- header section start -->
      <%- include('../partials/header.ejs')%>
      <!-- header section end -->


      <!-- about section start -->
      <div class="about_section layout_padding">
         <div class="container-fluid">
            <div class="row">
               <div class="col-md-6">
                  <div class="about_taital_main">
                     <h1 class="about_taital">About Us</h1>
                     <p class="about_text">
                        <h1>Team Members</h1>
                        <table border="3">
                           
                           <tr>
                              <th>Name</th>
                              <th>ID</th>
                              <th>E-mail</th>        
                           </tr>

                           <% if(members.length){
                              members.map((member)=> {%>
                           <tr>
                              <th><%=member.name%></th>
                              <th><%=member.member_id%></th>
                              <th><%=member.email%></th>        
                           </tr>
                           <%})}%>
                           
                     </table>
                     </p>
                  </div>
               </div>
               <div class="col-md-6 padding_right_0">
                  <div><img src="images/jeddah5.jpg" class="about_img"></div>
               </div>
            </div>
         </div>
      </div>
      <!-- about section end -->


      <!-- footer section start -->
      <%- include('../partials/footer.ejs')%>
      <!-- footer section end -->


      <!-- copyright section start -->
      <%- include('../partials/copyright.ejs')%>
      <!-- copyright section end -->  
   </body>
</html>